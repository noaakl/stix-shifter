{
  "name": "map-my-shifter",
  "version": "0.1.0",
  "homepage": "http://barvhaim.github.io/map-my-shifter",
  "private": true,
  "main": "dist/lib_index.js",
  "module": "dist/lib_index.js",
  "files": [ "dist", "README.md" ],
  "dependencies": {
    "@carbon/colors": "^10.14.0",
    "@carbon/ibm-security": "^1.45.0",
    "@carbon/icons-react": "^10.19.0",
    "@carbon/layout": "^10.13.0",
    "@carbon/type": "^10.16.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^7.1.2",
    "carbon-components": "10.41.0",
    "carbon-components-react": "7.41.0",
    "carbon-icons": "7.0.7",
    "file-saver": "^2.0.2",
    "node-sass": "4.12.0",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.4",
    "react-sticky-box": "^0.9.3",
    "redux": "^4.0.5",
    "uuid": "^8.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "prettier": "npx prettier --write --ignore-path .gitignore '**/*.{js,jsx,md,scss}'",
    "eslint": "npx eslint --ignore-path .gitignore --ext js,jsx .",
    "add": "git add ."
  },
  "pre-commit": [
    "prettier",
    "eslint",
    "add"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "gh-pages": "^3.1.0",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "react-redux": "^7.2.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1"
  }
}
